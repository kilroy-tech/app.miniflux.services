extends ./layout
block external_cdn_css
    link(rel='stylesheet', href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.6/dist/css/bootstrap.min.css")
    link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.css", integrity="sha512-aOG0c6nPNzGk+5zjwyJaoRUgCdOrfSDhmMID2u4+OIslr0GjpLKo7Xm0Ao3xmpM4T8AmIouRkqwj1nrdVsLKEQ==", crossorigin="anonymous", referrerpolicy="no-referrer")

block external_cdn_scripts
    script(src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous")
    script(src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js" integrity="sha256-VazP97ZCwtekAsvgPBSUwPFKdrwD3unUfSGVYrahUqU=" crossorigin="anonymous")
    script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.6/dist/js/bootstrap.bundle.min.js" integrity="sha384-j1CDi7MgGQ12Z7Qab0qlWQ/Qqz24Gc6BM0thvEMVjHnfYGF0rmFCozFSxQBxwHKO" crossorigin="anonymous")
    //- DEBUG items
    //- script(src="https://cdnjs.cloudflare.com/ajax/libs/gojs/2.1.30/go.js")
    script(src="https://cdn.jsdelivr.net/npm/gojs/release/go.js")
    
block inline_styles
    style.
        body { background: #2A303D !important; }

block title
    title Miniflux Pipeline Agents

block content
    .w-100.container-fluid
        #pd-palettes.w-75.mx-auto
            .card.mt-2.shadow
                .card-header
                    h3 Miniflux Pipeline Agents
                    p Click and copy individual agents, then paste into Pipeline Editor document.
                    p See "?" Help for details about configuring Miniflux for use with kilroy.ai.services
                .card-body
                    #pd-palette-area-1.palette-area.bg-white(style="min-height: 240px !important;")

block static_scripts
    script(src="./js/static/LocalStorageCommandHandler.js")

block custom_scripts
    script(src="./js/Figures.js")
    script(src="./js/palette_defs.js")
    script(src="./js/pipeline_palette_impl.js")