{
    "name": "miniflux.services/config/resume_hook",
    "version": "1",
    "description": "",
    "isInteractive": true,
    "clean_up": true,
    "inputs": [],
    "outputs": [
        "ta_connected_color"
    ],
    "args": {
        "role": "*"
    },
    "steps": [
        {
            "operation": "set_value",
            "isServer": true,
            "args": {
                "variable": "wfCleanupFlag",
                "value": "1"
            }
        },
        {
            "operation": "step_group",
            "isServer": true,
            "args": {
                "role": "system",
                "message": "",
                "timer": "24",
                "isInteractive": false,
                "canTerminate": false,
                "success": [
                    {
                        "operation": "swarm_command",
                        "isServer": true,
                        "args": {
                            "command": "join",
                            "command_args": {
                                "topic": "${ta_swarm}",
                                "alias": "${process_diagram_id}",
                                "webhook": "swarm_noop",
                                "seed": ""
                            }
                        }
                    },
                    {
                        "operation": "set_value",
                        "isServer": true,
                        "args": {
                            "variable": "ta_connected_color",
                            "value": "#00FF00"
                        }
                    }
                ]
            }
        },
        {
            "operation": "end",
            "isServer": true,
            "args": null
        }
    ],
    "externalForms": {}
}